        .ps {
            margin-bottom: 40px;
        }

            .ps a {
                display: inline-block;
                margin: 0 30px;
                width: 160px;
                height: 160px;
                overflow: hidden;
                border-radius: 50%;
            }

                .ps a img {
                    width: 100%;
                    filter: grayscale(100%);
                    transition: 0.4s all;
                }

                .ps a:hover > img {
                    filter: none;
                }

        .section {
            max-width: 600px;
            margin: auto;
            font-size: 20px;
            color: rgb(247, 247, 247);
            text-align: left;
            height: 0;
            overflow: hidden;
        }

            .section:target {
                height: auto;
            }

