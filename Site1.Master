            margin-bottom: 30px;
            text-align: center;
            text-transform: uppercase;
            font-size: 22px;
        }

        #ImgProfil {
            border-radius: 50%;
        }

        .bacc {
            position:relative;
            margin-top:50px;
            width: 100%;
            height: 250px;
            background: white;
            background-image: url(css/im/1.jpg);
            background-size: contain;
            animation: slide 30s infinite;
            margin-bottom: 50px;
            background-repeat: repeat;
            -webkit-box-shadow: 0px 7px 6px -3px rgba(168,168,168,1);
            -moz-box-shadow: 0px 7px 6px -3px rgba(168,168,168,1);
            box-shadow: 0px 7px 6px -3px rgba(168,168,168,1);
        }

        @keyframes slide {
            11.11% {
                background-image: url(css/im/1.jpg);
            }

            22.22% {
                background-image: url(css/im/2.jpg);
            }

            33.33% {
                background-image: url(css/im/3.jpg);
            }

            44.44% {
                background-image: url(css/im/4.jpg);
            }

            55.55% {
                background-image: url(css/im/5.jpg);
            }

            66.66% {
                background-image: url(css/im/6.jpg);
            }

            77.77% {
                background-image: url(css/im/7.jpg);
            }

            88.88% {
                background-image: url(css/im/8.jpg);
            }

            99.99% {
                background-image: url(css/im/9.jpg);
            }
        }
